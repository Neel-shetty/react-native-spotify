{
  "auth": {
    "SpotifyClone": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false,
      "frontendAuthConfig": {
        "socialProviders": [
          "GOOGLE",
          "AMAZON"
        ],
        "usernameAttributes": [],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_UPPERCASE"
          ]
        },
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "TOTP"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  }
}